{
	"name": "@armadacore/fnt",
	"version": "0.1.1",
	"description": "A collection of utility functions to make your TypeScript code more robust and safer.",
	"keywords": [
		"typescript",
		"utilities",
		"functional-programming",
		"typesafe"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/armadacore/fnt.git"
	},
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"lint:ts": "eslint src --ext .ts --ext .tsx --ignore-pattern *.test.ts && tsc --project tsconfig.json --noEmit",
		"type:check": "tsc --noEmit",
		"build": "tsc && vite build"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/node": "^22.10.2",
		"@vitest/coverage-istanbul": "^2.1.8",
		"@vitest/ui": "^2.1.8",
		"eslint": "^9.17.0",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.18.2",
		"vite": "^6.0.6",
		"vite-plugin-dts": "^4.4.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^2.1.8"
	}
}
